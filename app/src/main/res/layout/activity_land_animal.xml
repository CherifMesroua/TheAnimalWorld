<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LandAnimalLevels"
    android:background="#78BFEF"
    >

    <ImageView
        android:id="@+id/land1"
        android:layout_width="1151dp"
        android:layout_height="739dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.366"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.445"
        app:srcCompat="@drawable/land" />

    <ImageView
        android:id="@+id/level2"
        android:layout_width="186dp"
        android:layout_height="186dp"
        android:background="@drawable/bglevel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sahara"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/level3"
        android:layout_width="186dp"
        android:layout_height="186dp"
        android:background="@drawable/bglevel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.928"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.497"
        app:srcCompat="@drawable/jungle"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/level1"
        android:layout_width="186dp"
        android:layout_height="186dp"
        android:background="@drawable/bglevel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.082"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.502"
        app:srcCompat="@drawable/farmbglevel"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="185dp"
        android:layout_height="71dp"
        android:layout_alignParentBottom="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.926"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8"
        app:srcCompat="@drawable/emptystars" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="185dp"
        android:layout_height="71dp"
        android:layout_alignParentBottom="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8"
        app:srcCompat="@drawable/emptystars" />

    <ImageView
        android:id="@+id/stars_level1"
        android:layout_width="185dp"
        android:layout_height="71dp"
        android:layout_alignParentBottom="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.082"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8"
        app:srcCompat="@drawable/emptystars" />

    <ImageView
        android:id="@+id/lock1"
        android:layout_width="100dp"
        android:layout_height="97dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/level3"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="@+id/level3"
        app:layout_constraintTop_toTopOf="@+id/textView"
        app:layout_constraintVertical_bias="0.789"
        app:srcCompat="@drawable/lock" />

    <ImageView
        android:id="@+id/lock2"
        android:layout_width="100dp"
        android:layout_height="97dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/level2"
        app:layout_constraintStart_toStartOf="@+id/level2"
        app:layout_constraintTop_toTopOf="@+id/level2"
        app:layout_constraintVertical_bias="0.789"
        app:srcCompat="@drawable/lock" />

    <ImageView
        android:id="@+id/Wsetting"
        android:layout_width="89dp"
        android:layout_height="91dp"
        android:onClick="show_setting"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.979"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.039"
        app:srcCompat="@drawable/setting" />

    <LinearLayout
        android:id="@+id/settingLayout"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bgtrasparent"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">
    <LinearLayout
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="56dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/music_on" />

        <SeekBar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="20dp"
            android:thumbTint="@color/fireorange"
            android:progressTint="@color/fireorange"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="56dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/volume_on" />

        <SeekBar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="20dp"
            android:thumbTint="@color/fireorange"
            android:progressTint="@color/fireorange"
            />
    </LinearLayout>
    </LinearLayout>

    <TextView
        android:layout_width="170sp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="حيوانات المزرعة"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/stars_level1"
        app:layout_constraintEnd_toEndOf="@+id/level1"
        app:layout_constraintStart_toStartOf="@+id/level1"
        app:layout_constraintTop_toTopOf="@+id/level1"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="170sp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="حيوانات الصحراء"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/level2"
        app:layout_constraintStart_toStartOf="@+id/level2"
        app:layout_constraintTop_toTopOf="@+id/level2"
        app:layout_constraintVertical_bias="0.007" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="170sp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="حيوانات اللأدغال"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/level3"
        app:layout_constraintHorizontal_bias="0.437"
        app:layout_constraintStart_toStartOf="@+id/level3"
        app:layout_constraintTop_toTopOf="@+id/level3"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>